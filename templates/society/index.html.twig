{% extends 'base.html.twig' %}

{% block title %}La liste des sociétés!{% endblock %}

{% block body %}

{% if society %}
<h1> Liste des sociétés</h1>
<table class="table table-striped"> 
<thead>
<tr>
<th>id</th>
<th>phone</th>
<th>name</th>
<th>type</th>
<th>website</th>
<th>Actions</th>
</tr>
</thead>
<tbody> 

{% for society in society %}
<tr> 
<td> {{ society.id }} </td>
<td> {{ society.phone }} </td>
<td> {{ society.name }} </td>
<td> {{ society.type }} </td>
<td> {{ society.website }} </td>
<td> 
<a href="/society/{{ society.id }}" class="btn 
btn-dark">Voir</a>
</td>
</tr>
 {% endfor %}
</tbody>
</table>
{% else  %}
<p> La liste des sociétés est vide</p>

{% endif %}

{% endblock %}
